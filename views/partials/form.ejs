<div class="register-title text-center"><%= title %></div>
<div class="container form_container">
    <div class="row">
        <form action="/post_<%=page%>" method="post">
            <div class="col-lg-12">
                <div class="well well-sm"><strong><span class="glyphicon glyphicon-asterisk"></span>Required Field</strong></div>
              		<label>Name</label>
                    <div class="input-group">
                        <input type="text" class="form-control" name="InputName" placeholder="Enter Name" autofocus required>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
                    <label>Email</label>
                    <div class="input-group">
                        <input type="email" class="form-control" name="InputEmail" placeholder="abc@gmail.com" required>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-minus"></span></span>
                    </div>
					<label>Phone</label>
                    <div class="input-group">
                        <input type="text" class="form-control" name="InputPhone" placeholder="----------">
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
                    <label>Address</label>
                    <div class="input-group">
                        <textarea name="InputAddress" class="form-control" rows="5" required></textarea>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
                <input type="submit" name="Submit" class="btn btn-lg btn-danger pull-right">
            </div>
        </form>
    </div>
</div>


